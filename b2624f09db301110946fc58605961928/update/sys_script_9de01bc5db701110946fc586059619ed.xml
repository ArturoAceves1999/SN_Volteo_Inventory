        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	var gr = new GlideRecord('x_776815_inventory_m2m_inventory_changes_v2');
	gr.initialize();
	gr.product = current.sys_id;
	if (current.quantity > previous.quantity ){
		gr.quantity = current.quantity - previous.quantity ;
		gr.type = 'refil';
	}else { 
		gr.quantity = previous.quantity - current.quantity;
		gr.type = 'grab';
	}
	
	gr.insert();

})(current, previous);]]></script>
